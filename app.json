{
  "name": "AGORA-MUSIC",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/MR-AGORA/AGORA-MUSIC",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string from @agora_ssgenbot",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": "Agora Music Bot"
    },
    "API_ID": {
      "description": "App ID from @agora_apibot",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from @agora_apibot ",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "60"
    }
  }
}
